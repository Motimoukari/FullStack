<h1 class = "text-center mb-3">{{title}}</h1>
<form action="/api/members" method="POST" class="mb-4"></form>
    <div class="form-group">
            <label for="name">Name</label>
                <input type="text" name="name" id="form-control">
    </div>

    <div class="form-group">
            <label for="email">email</label>
                <input type="email" name="email" id="form-control">
    </div>
    <input type="submit" name="Add member" class="btn btn-primary btn-block">

<h4>Members</h4>

<ul class="list-group">
    {{#each members}}
        <li class="list-group-item"> 
            {{this.name}}: {{this.email}}
        </li>
    {{/each}}
</ul>